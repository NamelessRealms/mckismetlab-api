{
  "name": "mckismetlab-api",
  "version": "1.4.0",
  "description": "mcKismetLab API Service",
  "main": "./build/index.js",
  "scripts": {
    "start": "cross-env NODE_ENV=production node ./dist/index.js",
    "dev": "cross-env NODE_ENV=development ts-node-dev --respawn --watch --files -- ./src/index.ts",
    "build": "yarn outputVersion && yarn buildDir && tsc",
    "buildDir": "node build.js",
    "outputVersion": "node -p \"'export const API_VERSION = ' + JSON.stringify(require('./package.json').version) + ';'\" > src/version.ts"
  },
  "author": "Quasi",
  "license": "GPL-3.0",
  "dependencies": {
    "cheerio": "^1.0.0-rc.10",
    "cookie-parser": "^1.4.5",
    "cross-env": "^7.0.3",
    "csurf": "^1.11.0",
    "dotenv": "^10.0.0",
    "ejs": "^3.1.6",
    "express": "^4.17.1",
    "express-mysql-session": "^2.1.6",
    "express-session": "^1.17.2",
    "form-data": "^4.0.0",
    "fs-extra": "^10.0.0",
    "got": "^11.8.2",
    "helmet": "^4.6.0",
    "jsonwebtoken": "^8.5.1",
    "morgan": "^1.10.0",
    "multer": "^1.4.4",
    "mysql2": "^2.2.5",
    "tslib": "^2.3.1",
    "uniqid": "^5.3.0"
  },
  "devDependencies": {
    "@types/cookie-parser": "^1.4.2",
    "@types/csurf": "^1.11.2",
    "@types/express": "^4.17.13",
    "@types/express-mysql-session": "^2.1.3",
    "@types/express-session": "^1.17.4",
    "@types/fs-extra": "^9.0.12",
    "@types/jsonwebtoken": "^8.5.4",
    "@types/morgan": "^1.9.3",
    "@types/multer": "^1.4.7",
    "@types/mysql2": "github:types/mysql2",
    "@types/node": "^16.4.5",
    "@types/uniqid": "^5.3.1",
    "ts-node": "^10.1.0",
    "ts-node-dev": "^1.1.8",
    "typescript": "^4.3.5"
  }
}
